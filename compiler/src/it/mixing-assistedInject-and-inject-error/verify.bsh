new BuildLogValidator().assertHasText(buildLog, new String[]{
    "A constructor cannot be annotated with both @Inject and @AssistedInject: test.TestApp.TestClass1.TestClass1()"});
new BuildLogValidator().assertHasText(buildLog, new String[]{
    "A constructor cannot be annotated with both @Inject and @AssistedInject: test.TestApp.TestClass2.TestClass2()"});